{
  "hosting": {
    "public": "dist/Body-fitness-Gym",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source": "//*",
      "destination": "https://spring-body-fitness-gym.herokuapp.com",
      "type": 301
    }],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
